@page
@model BLSystem.Pages.HoldPage.CreateHoldModel
@{
    ViewData["Titel"] = "Create";
}

<h1>Opret</h1>
<div class="row">
    <div class=" col-md-4">
        <form method="post">
            <div asp-validation-summary=" ModelOnly" class=" text-danger">
            </div>

            <div class=" form-group">
                <label asp-for="@Model.hold.Hold_Id " class=" control_label"></label>
                <input asp-for="@Model.hold.Hold_Id" class="form-control" />
                <span asp-validation-for="@Model.hold.Hold_Id" class=" text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Opret" class="btn btn-primary" />
            </div>

        </form>
        <div>
            <a asp-page="GetHold">Tilbage </a>
        </div>
    </div>
</div>
